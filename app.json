{
  "expo": {
    "name": "eggTimer",
    "slug": "eggTimer",
    "version": "3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/eggTimer.png",
      "resizeMode": "contain",
      "backgroundColor": "#222831"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#222831"
      },
      "package": "com.brijenmakwana.eggTimer",
      "permissions" : []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
